RewriteEngine on

#
# Place Rules Below
#

RewriteBase /services/

# misc WordPress rewrites
RewriteRule ^/whois/(.*)$ /whois.svc/$1
RewriteRule ^/amazon/(.*)$ /amazon.svc/$1
RewriteRule ^/resume/(.*)$ /resume.svc/$1
RewriteRule ^(.*)\.xml(.*)$ $1/xml$2
RewriteRule ^(.*)\.json(.*)$ $1/json$2